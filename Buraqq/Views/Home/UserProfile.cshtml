@using Buraqq.Models.ViewModel
@model List<TeacherInfoViewModel>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<div>
    <h1>Profile</h1>
</div>
@foreach (var item in Model)
{
    <div class="row">
        <div class="col-md-6">
            <div class="list-group">
                <h5><b>Name</b>@item.Name</h5>
                <h5><b>Fee</b>@item.Fee</h5>
                <h5><b>Description</b>@item.Description</h5>
                <h5><b>Subject</b>@item.Subject</h5>
            </div>
        </div>
    </div>
}
